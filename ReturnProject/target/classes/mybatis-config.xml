<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias type="kr.co.three.main.dto.MainDTO" alias="main" />
		<typeAlias type="kr.co.three.member.dto.MemberDTO"
			alias="member" />
		<typeAlias type="kr.co.three.admin.dto.adminDTO"
			alias="admin" />
		<typeAlias type="kr.co.three.sales.dto.SalesDTO"
			alias="sales" />
		<typeAlias type="kr.co.three.board.dto.boardDTO"
			alias="board" />
		<typeAlias type="kr.co.three.reply.dto.ReplyDTO"
			alias="reply" />
		<typeAlias type="kr.co.three.pay.dto.PayDTO" alias="pay" />
	</typeAliases>

	<mappers>
		<mapper resource="/mappers/main/main-mapper.xml" />
		<mapper resource="/mappers/member/member-mapper.xml" />
		<mapper resource="/mappers/sales/sales-mapper.xml" />
		<mapper resource="/mappers/admin/admin-mapper.xml" />
		<mapper resource="/mappers/board/board-mapper.xml" />
		<mapper resource="/mappers/reply/reply-mapper.xml" />
		<mapper resource="/mappers/pay/pay-mapper.xml" />
	</mappers>
</configuration>




